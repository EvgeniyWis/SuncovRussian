{
  "name": "suncov-russian",
  "version": "1.0.0",
  "homepage": "https://kriswis.github.io/SuncovRussian/",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "deploy": "gh-pages -d build",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "storybook dev -p 6006 --ci",
    "test:ui": "npx loki test --port 6007 & npm run test:ui:report",
    "test:ui:ok": "npx loki approve",
    "test:ui:report:json": "node scripts/generate-visual-json-report.cjs",
    "test:ui:report:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:unit": "jest --config ./config/jest/jest.config.ts"
  },
  "keywords": [],
  "author": "KrisWis",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/webpack-dev-server": "^4.7.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/mini-css-extract-plugin": "^2.5.0",
    "@types/react-dom": "^19.0.1",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-ulbi-tv-plugin": "^0.0.10",
    "gh-pages": "^6.2.0",
    "globals": "^15.13.0",
    "html-webpack-plugin": "^5.6.3",
    "loki": "^0.35.1",
    "react-refresh": "^0.16.0",
    "reg-cli": "^0.18.9",
    "storybook": "^8.4.7",
    "stylelint": "^16.11.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  },
  "overrides": {
    "loki": {
      "@storybook/react": "$@storybook/react"
    }
  }
}
